{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "fileTypes": ["fish"],
  "firstLineMatch": "^#!.*\\bfish\\b",
  "foldingStartMarker": "^\\s*(function|while|if|switch|for|begin)\\s.*$",
  "foldingStopMarker": "^\\s*end\\s*$",
  "keyEquivalent": "^~F",
  "name": "fish",
  "scopeName": "source.fish",
  "uuid": "874b33e9-2128-4cb1-b4c9-ae6e27ba701c",
  "patterns": [{ "include": "#main" }],
  "repository": {
    "main": {
      "name": "bmalehorn.main",
      "patterns": [
        { "include": "#atom" },
        { "include": "#word" },
        { "include": "#number" }
      ]
    },
    "word": {
      "name": "word",
      "match": "\\b[a-zA-Z]+\\b"
    },
    "number": {
      "name": "number",
      "match": "\\b[0-9]+\\b"
    },
    "atom": {
      "name": "atom",
      "match": "\\b[a-zA-Z0-9]+\\b"
    }
  }
}
